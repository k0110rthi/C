#include<iostream>


using namespace std;

class stud
{
    int a,b;
    public:
    
    stud(int x,int y)
    {
        a=x;
        b=y;
    }
    void display()
    {
        cout<<"values of a and b:"<<a<<"\t"<<b;
    }
};
    int main()
    {
        stud s1(10,20);
        s1.display();
        return 0;
    }
    
    
    
    
    #include<iostream>

using namespace std;

class cube
{

    public:
       int a;
    cube()
    {
        a=10;
    }
    
};
int main()
{
    cube c;
    cout<<c.a;
    return 0;
}



#include<iostream>
using namespace std;
class Base
{ int x;
  public:
  Base() { cout << "Base default constructor"<< endl; }
};

class Derived : public Base
{ int y;
  public:
  Derived()
  {
      cout << "Derived default constructor"<<endl;
 }
  Derived(int i)  
  {
      cout << "Derived parameterized constructor"<<endl; }
  };

int main()
{
        
 Derived d1;    
 Derived d2(10);
}



